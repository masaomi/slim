<div class="panel panel-default">
  <div class="panel-heading"><h3 class="panel-title">Score distribution</h3></div>
  <div class="panel-body">

    <%= column_chart @score_dist,
                     :library => {
                             :hAxis => {:title => "Score Distribution"}
                     }
    %>
  </div><div class="panel-footer"><% if @score_list %>
      range: <%= @score_list[0] %> - <%= @score_list[1] %>, ave: <%= "%.1f" % @score_list[2] %>, sd: <%= "%.1f" % @score_list[3] %><br \>
  <% end %></div></div>


